/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Tand(a=in, b=sel, out=b);
    Tnot(a=sel, out=nsel);
    Tand(a=in, b=nsel, out=a);

}
